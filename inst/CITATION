citHeader("To cite the micromap package in publications use:")

citEntry(
   entry="Manual",
   title="micromap: Linked Micromap Plots",
   author="Quinn Payton and Anthony R. Olsen",
   year=2013,
   note="R package version 1.6",
   url="http://www.epa.gov/nheerl/arm/",
   textVersion=paste("Payton, Q. and Olsen, A. R. (2012).",
                      "micromap: Linked Micromap Plots.",
                      "R package version 1.6.",
                      "URL: http://www.epa.gov/nheerl/arm/.")
)
